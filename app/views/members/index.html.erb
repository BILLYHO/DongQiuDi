<h1>Listing members</h1>

<table>
  <thead>
    <tr>
      <th>Member ID</th>
      <th>Name</th>
      <th>Gender</th>
      <th>Position</th>
      <th>About</th>
      <th>Motto</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <div class="col-md-8">
    <% if @members.any? %>
        <h3>Members (<%= @members.count %>)</h3>
        <ol class="members">
          <%= render @members %>
        </ol>
        <%= will_paginate @members %>
    <% end %>
  </div>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
